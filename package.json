{
  "name": "chatgpt_discord_workers",
  "module": "src/index.ts",
  "type": "module",
  "scripts": {
    "start": "wrangler dev",
    "lint": "bunx @biomejs/biome lint src",
    "lint:fix": "bunx @biomejs/biome lint --write src",
    "format": "bunx @biomejs/biome format src",
